{
  "name": "candidator-dashboard",
  "version": "1.0.0",
  "description": "Candidator Dashboard Application",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "deploy": "concurrently --kill-others \"serve -s client\\dist\" \"node server/index.js\"",
    "dev": "concurrently --kill-others \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "install-server": "npm install",
    "postinstall": "npm run install-client",
    "render": "npm run build && npm run deploy"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.53.0",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "googleapis": "^154.1.0",
    "jsonwebtoken": "^9.0.2",
    "open": "^10.2.0",
    "pg": "^8.11.3",
    "promise-request-retry": "^1.0.2",
    "serve": "^14.2.4"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}
