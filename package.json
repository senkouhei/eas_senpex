{
  "name": "candidator-dashboard",
  "version": "1.0.0",
  "description": "Candidator Dashboard Application",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "deploy": "node server/index.js",
    "dev": "concurrently --kill-others \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "install-server": "npm install",
    "postinstall": "npm run install-client",
    "render": "npm run build && npm run deploy"
  },
  "dependencies": {
    "@hyzyla/pdfium": "^2.1.9",
    "@supabase/supabase-js": "^2.53.0",
    "axios": "^1.11.0",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "googleapis": "^154.1.0",
    "mammoth": "^1.10.0",
    "open": "^10.2.0",
    "openai": "^5.12.2",
    "pdf-parse": "^1.1.1",
    "sharp": "^0.34.3",
    "tesseract.js": "^6.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}
