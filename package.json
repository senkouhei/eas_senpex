{
  "name": "candidator-dashboard",
  "version": "1.0.0",
  "description": "Candidator Dashboard Application",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "deploy": "node server/index.js",
    "dev": "concurrently --kill-others \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "install-server": "npm install",
    "render": "npm run build && npm run deploy",
    "start-bots": "node server/bot/bot_manager.js",
    "start-bot-workers": "node server/bot/bot_worker.js"
  },
  "dependencies": {
    "@hyzyla/pdfium": "^2.1.9",
    "@supabase/supabase-js": "^2.53.0",
    "axios": "^1.11.0",
    "bullmq": "^5.58.4",
    "chartjs-plugin-datalabels": "^2.2.0",
    "cheerio": "^1.1.2",
    "chromium": "^3.0.3",
    "cors": "^2.8.5",
    "country-list": "^2.3.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "googleapis": "^154.1.0",
    "mailparser": "^3.7.4",
    "mammoth": "^1.10.0",
    "moment": "^2.30.1",
    "node-imap": "^0.9.6",
    "open": "^10.2.0",
    "openai": "^5.12.2",
    "pdf-parse": "^1.1.1",
    "puppeteer": "^24.16.2",
    "puppeteer-core": "^24.16.2",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sharp": "^0.34.3",
    "tesseract.js": "^6.0.1",
    "twilio": "^5.8.0",
    "us-state-codes": "^1.1.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}
